    "jsonwebtoken": "^9.0.0",
    "moment-js": "^1.1.15",
    "mongoose": "^5.3.13",
    "nodemon": "^1.18.7",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
